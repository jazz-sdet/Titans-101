
arr = {1,  45, 333, 8 }

1*100= 100+45 = 145

145 * 1000= 145000 + 333 = 145333* 10 + 8 = 1453338 

int result=arr[0]; 

for(int i=1;i<arr.length;i++){
	
	int count=0; 
	int num=arr[i]; 
	int mult=1; 

	while(num>0){

		num/=10; 
		count++; 
	}

	// count = 2; 
	for(int i=0;i<count;i++){
		mult*=10; 
	}

	// mult=100; 

		result*=mult+arr[i]; 

}


return result; 


