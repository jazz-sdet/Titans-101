	MAIN METHOD WHAT IS IT




	package Variables;

public class Hello {

    public static void main(String [] args){

        int cows=23;
        int chickens=42;
        int totalOfAnimals=cows+chickens;

        int cowsLegNum=cows*4;
        int chickenLegNum=chickens*2;
        int totalNumLegs=cowsLegNum+chickenLegNum;
        System.out.println("Total number of legs in farm " +totalNumLegs);

        int totalCowPrice=2000*cows;
        int totalChickenPrice=11*chickens;
        int totalAnimalPrice=totalCowPrice+totalChickenPrice;
        System.out.println("Total of animal  " + totalOfAnimals );
        System.out.println("Total of animal price is $ " +totalAnimalPrice);
    }
}
